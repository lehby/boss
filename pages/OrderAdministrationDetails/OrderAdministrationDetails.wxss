.state{
    height: 140rpx;
    line-height: 140rpx;
    padding: 0 20rpx;
    font-size: 28rpx;
    font-weight: 900;
    display: flex;
    justify-content: space-between;
    background-color: #fff;   
}
.InventoryGoods{
    padding: 20rpx 20rpx;
    font-size: 28rpx;
    background-color: #fff;
}
.InventoryTitle{
    padding: 28rpx 0;
    font-weight: 900;
}
.from_top{
    height: 64rpx;
    background-color: #EDEDED;
    display: flex;
    justify-content: space-around;
}
.from_top>view{
    width: 25%;
    height: 64rpx;
    line-height: 64rpx;
    text-align: center;
}
.from_td{
    height: 88rpx;
    outline: 2rpx solid #EDEDED;
    display: flex;
    justify-content: space-around;
}
.from_td>view{
    width: 25%;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    color: #999;
}
.information{
    display: flex;
    justify-content: space-between;
    height: 90rpx;
    line-height: 90rpx;
    padding: 0 20rpx;
    background-color: #fff;
    font-size: 28rpx;
    border-bottom: 2rpx solid #eee;
}
.InventoryName{
    margin-top: 22rpx;
    padding: 34rpx 20rpx;
    background-color: #fff;
    font-size: 28rpx;
}