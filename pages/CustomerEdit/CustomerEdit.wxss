.InventoryGoods{
    padding: 20rpx 20rpx;
    font-size: 28rpx;
    background-color: #fff;
}
.InventoryTitle{
    padding: 28rpx 0;
    font-weight: 900;
}
.from_top{
    height: 64rpx;
    background-color: #EDEDED;
    display: flex;
    justify-content: space-around;
}
.from_top>view{
    width: 50%;
    height: 64rpx;
    line-height: 64rpx;
    text-align: center;
}
.from_td{
    height: 88rpx;
    outline: 2rpx solid #EDEDED;
    display: flex;
    justify-content: space-around;
}
.from_td>view{
    width: 50%;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    color: #999;
}
.information{
    display: flex;
    justify-content: space-between;
    height: 90rpx;
    line-height: 90rpx;
    padding: 0 20rpx;
    background-color: #fff;
    font-size: 28rpx;
    border-bottom: 2rpx solid #eee;
}
.information_ipt{
    display: flex;
    height: 90rpx;
    line-height: 90rpx;
    padding: 0 20rpx;
    background-color: #fff;
    font-size: 28rpx;
    border-bottom: 2rpx solid #eee;
}

.ipt>input{
    margin-top:20rpx;
    margin-left:50rpx;
}
.InventoryName{
    margin-top: 22rpx;
    padding: 34rpx 20rpx;
    background-color: #fff;
    font-size: 28rpx;
}
.btn_edit{
    padding: 0 20rpx;
    background-color: #fff;
    height: 100rpx;
    display: flex;
    justify-content: flex-end;
}
.edit{
    width: 160rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    background-color: #2168d3;
    color: #fff;
    border-radius: 50rpx;
    margin-top: 20rpx;
}
.btn_Submission{
    width: 600rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    border-radius: 50rpx;
    margin: 50rpx auto;
    background-color: #2168d3;
    color: #fff;
}