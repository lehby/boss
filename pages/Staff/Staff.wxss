.footer{
    width: 100%;
    height: 100rpx;
    background-color: #0E58BC;
    color: #fff;
    position:fixed;
    bottom: 0rpx;
    line-height: 100rpx;
    text-align: center;
}
.operatorList{
    padding: 0 20rpx;
    background-color: #fff;
    margin-top: 20rpx;
    font-size: 28rpx;
}
.operatorList_top{
    padding: 15rpx 0;
    border-bottom: 2rpx solid #f5f5f5;
}
.operatorList_top>view{
    display: flex;
    line-height: 60rpx;
}
.text_left{
    padding: 5rpx 0;
    width: 144rpx;
    color: #A0A0A0;
}
.text_right{
    padding: 5rpx 0;
}
.text_right>input{
    height:60rpx;
}
.operatorList_bottom{
    height: 100rpx;
    display: flex;
    justify-content: flex-end;
}
.btn{
    width: 160rpx;
    height: 64rpx;
    background-color: #0E58BC;
    color: #fff;
    line-height: 64rpx;
    text-align: center;
    border-radius: 50rpx;
    margin-top:16rpx;
}
.occupy{
    width: 100%;
    height: 100rpx;
}