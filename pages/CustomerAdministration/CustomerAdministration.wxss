.CustomerList{
    padding: 0 20rpx;
    font-size: 28rpx;
    margin-top: 20rpx;
    background-color: #fff;
}
.text{
    display: flex;
}
.text_left{
    color: #9b9b9b;
}
.text_right{
    margin-left: 30rpx;
}
.text_list{
    padding: 30rpx 0;
    border-bottom: 2rpx solid #f5f5f5;
}
.btn_edit{
    height: 100rpx;
    display: flex;
    justify-content: flex-end;
}
.edit{
    width: 160rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    background-color: #2168d3;
    color: #fff;
    border-radius: 50rpx;
    margin-top: 20rpx;
}